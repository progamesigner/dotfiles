{
    "bookmarks.navigateThroughAllFiles": true,
    "bookmarks.saveBookmarksInProject": false,
    "colorHelper.disableTransparent": true,
    "docker.showExplorer": false,
    "editor.autoIndent": true,
    "editor.find.globalFindClipboard": true,
    "editor.fontFamily": "Source Code Pro",
    "editor.fontSize": 18,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.rulers": [
        50,
        80,
        100
    ],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "gitlens.advanced.messages": {"suppressShowKeyBindingsNotice": true},
    "gitlens.codeLens.scopes": [
        "blocks",
        "containers",
        "document"
    ],
    "gitlens.gitExplorer.enabled": false,
    "gitlens.historyExplorer.enabled": false,
    "gitlens.keymap": "alternate",
    "latex-workshop.view.pdf.viewer": "tab",
    "plantuml.render": "PlantUMLServer",
    "plantuml.server": "http://local.progamesigner.com:8800",
    "rust-client.channel": "stable",
    "search.smartCase": true,
    "search.useIgnoreFiles": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "window.restoreWindows": "all",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Spacegray Ocean Dark",
    "workbench.commandPalette.preserveInput": true,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "vs-minimal"
}
