{
    "colorHelper.disableTransparent": true,
    "editor.find.globalFindClipboard": true,
    "editor.fontFamily": "Source Code Pro",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.renderControlCharacters": true,
    "editor.rulers": [
        50,
        80,
        100
    ],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "gitlens.keymap": "alternate",
    "leetcode.defaultLanguage": "csharp",
    "plantuml.render": "PlantUMLServer",
    "rust.all_features": true,
    "search.globalFindClipboard": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useIgnoreFiles": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.macOptionIsMeta": true,
    "window.restoreWindows": "all",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Spacegray Ocean Dark",
    "workbench.commandPalette.preserveInput": true,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "vs-minimal",
    "git-graph.showStatusBarItem": false,
    "logViewer.showStatusBarItemOnChange": true,
    "files.eol": "\n",
    "plantuml.previewSnapIndicators": true,
    "breadcrumbs.enabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
    "auto-close-tag.SublimeText3Mode": true,
    "python.jediEnabled": false
}
